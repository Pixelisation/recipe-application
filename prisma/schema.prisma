// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id Int @id @default(autoincrement())
}

model Session {
  id Int @id @default(autoincrement())
}

model verification {
  identifier String
  token      String @unique
}

model User {
  id Int @id @default(autoincrement())
}

model Recipes{
  id Int @id @default(autoincrement())
}
